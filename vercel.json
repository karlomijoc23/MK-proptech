{
  "buildCommand": "corepack yarn --cwd frontend install && corepack yarn --cwd frontend build",
  "outputDirectory": "frontend/build",
  "framework": null,
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://mk-proptech-backend.onrender.com/api/$1"
    }
  ]
}
